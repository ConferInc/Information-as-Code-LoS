      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_EMDYes_di" bpmnElement="Flow_EMDYes">
        <di:waypoint x="700" y="180" />
        <di:waypoint x="800" y="180" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="741" y="162" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_CalcToSuff_di" bpmnElement="Flow_CalcToSuff">
        <di:waypoint x="900" y="180" />
        <di:waypoint x="950" y="180" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_SuffNo_di" bpmnElement="Flow_SuffNo">
        <di:waypoint x="975" y="205" />
        <di:waypoint x="975" y="340" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="983" y="260" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_FundsAdded_di" bpmnElement="Flow_FundsAdded">
        <di:waypoint x="925" y="380" />
        <di:waypoint x="900" y="380" />
        <di:waypoint x="900" y="180" />
        <di:waypoint x="950" y="180" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_SuffYes_di" bpmnElement="Flow_SuffYes">
        <di:waypoint x="1000" y="180" />
        <di:waypoint x="1102" y="180" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1042" y="162" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>