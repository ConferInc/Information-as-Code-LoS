      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_LargeDep_di" bpmnElement="Gateway_LargeDep" isMarkerVisible="true">
        <dc:Bounds x="500" y="155" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="488" y="118" width="75" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_SourceDep_di" bpmnElement="Task_ReviewLargeDeposit">
        <dc:Bounds x="475" y="340" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_EMD_di" bpmnElement="Gateway_EMDCleared" isMarkerVisible="true">
        <dc:Bounds x="650" y="155" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="638" y="131" width="74" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_VerifyEMD_di" bpmnElement="Task_VerifyEMD">
        <dc:Bounds x="625" y="340" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_CalcFunds_di" bpmnElement="Task_CalcFunds">
        <dc:Bounds x="800" y="140" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_Suff_di" bpmnElement="Gateway_Sufficient" isMarkerVisible="true">
        <dc:Bounds x="950" y="155" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="933" y="131" width="85" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Insuff_di" bpmnElement="Task_InsufficientFunds">
        <dc:Bounds x="925" y="340" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_Assets_di" bpmnElement="EndEvent_AssetsVerified">
        <dc:Bounds x="1102" y="162" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1084" y="205" width="73" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_StartToOCR_di" bpmnElement="Flow_StartToOCR">
        <di:waypoint x="288" y="180" />
        <di:waypoint x="340" y="180" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_OCRToLargeDep_di" bpmnElement="Flow_OCRToLargeDep">
        <di:waypoint x="440" y="180" />
        <di:waypoint x="500" y="180" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_DepYes_di" bpmnElement="Flow_DepYes">
        <di:waypoint x="525" y="205" />
