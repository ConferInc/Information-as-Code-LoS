<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:bioc="http://bpmn.io/schema/bpmn/biocolor/1.0" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_CreditReview" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.43.1" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.4.0">
  <bpmn:collaboration id="Collaboration_CreditReview">
    <bpmn:participant id="Participant_CreditProc" name="Processing - Credit Review (L2)" processRef="Process_CreditReview_L2" />
  </bpmn:collaboration>
  <bpmn:process id="Process_CreditReview_L2" name="Credit Review Process" isExecutable="true">
    <bpmn:laneSet id="LaneSet_Credit">
      <bpmn:lane id="Lane_Processor_Credit" name="Loan Processor (Human)">
        <bpmn:flowNodeRef>Task_ReviewInquiryLOX</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ManageDerogatory</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReviewAuthUser</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Agents_Credit" name="System Agents (Automated)">
        <bpmn:flowNodeRef>StartEvent_CreditReport</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AnalyzeCredit</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Disputes</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_OrderSupplement</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Inquiries</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Derogatory</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_CreditCleared</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_CreditReport" name="Credit Report Received">
      <bpmn:outgoing>Flow_StartToAnalyze</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_AnalyzeCredit" name="Analyze Credit Report">
      <bpmn:documentation>Extracts inquiries, disputes, derogatory items.</bpmn:documentation>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="analyze-credit-report" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_StartToAnalyze</bpmn:incoming>
      <bpmn:outgoing>Flow_AnalyzeToDisputes</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_Disputes" name="Disputed Accounts?">
      <bpmn:documentation>Are there active disputes on tradelines?</bpmn:documentation>
      <bpmn:incoming>Flow_AnalyzeToDisputes</bpmn:incoming>
      <bpmn:outgoing>Flow_DisputeYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_DisputeNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_OrderSupplement" name="Order Credit Supplement (Remove Dispute)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="order-credit-supplement" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_DisputeYes</bpmn:incoming>
      <bpmn:outgoing>Flow_SuppToInquiry</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_Inquiries" name="Inquiries &#60; 120 Days?">
      <bpmn:incoming>Flow_DisputeNo</bpmn:incoming>
      <bpmn:incoming>Flow_SuppToInquiry</bpmn:incoming>
      <bpmn:incoming>Flow_InquiryResolved</bpmn:incoming>
      <bpmn:outgoing>Flow_InquiryYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_InquiryNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_ReviewInquiryLOX" name="Review Inquiry LOX">
      <bpmn:documentation>Confirm no new debt or add debt.</bpmn:documentation>
      <bpmn:incoming>Flow_InquiryYes</bpmn:incoming>
      <bpmn:outgoing>Flow_InquiryResolved</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_Derogatory" name="Derogatory Items?">
      <bpmn:documentation>Collections, Charge-offs, Judgments?</bpmn:documentation>
      <bpmn:incoming>Flow_InquiryNo</bpmn:incoming>
      <bpmn:incoming>Flow_DerogResolved</bpmn:incoming>
      <bpmn:outgoing>Flow_DerogYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_DerogNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_ManageDerogatory" name="Manage Derogatory Items">
      <bpmn:documentation>Obtain payoff proof or explanation.</bpmn:documentation>
      <bpmn:incoming>Flow_DerogYes</bpmn:incoming>
      <bpmn:outgoing>Flow_DerogResolved</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_ReviewAuthUser" name="Review Authorized Users">
      <bpmn:documentation>Exclude if not borrower's debt.</bpmn:documentation>
      <bpmn:incoming>Flow_DerogNo</bpmn:incoming>
      <bpmn:outgoing>Flow_AuthUserEnd</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_CreditCleared" name="Credit Review Complete">
      <bpmn:incoming>Flow_AuthUserEnd</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_StartToAnalyze" sourceRef="StartEvent_CreditReport" targetRef="Task_AnalyzeCredit" />
    <bpmn:sequenceFlow id="Flow_AnalyzeToDisputes" sourceRef="Task_AnalyzeCredit" targetRef="Gateway_Disputes" />
    <bpmn:sequenceFlow id="Flow_DisputeYes" name="Yes" sourceRef="Gateway_Disputes" targetRef="Task_OrderSupplement" />
    <bpmn:sequenceFlow id="Flow_DisputeNo" name="No" sourceRef="Gateway_Disputes" targetRef="Gateway_Inquiries" />
    <bpmn:sequenceFlow id="Flow_SuppToInquiry" sourceRef="Task_OrderSupplement" targetRef="Gateway_Inquiries" />
    <bpmn:sequenceFlow id="Flow_InquiryYes" name="Yes" sourceRef="Gateway_Inquiries" targetRef="Task_ReviewInquiryLOX" />
    <bpmn:sequenceFlow id="Flow_InquiryResolved" sourceRef="Task_ReviewInquiryLOX" targetRef="Gateway_Inquiries" />
    <bpmn:sequenceFlow id="Flow_InquiryNo" name="No" sourceRef="Gateway_Inquiries" targetRef="Gateway_Derogatory" />
    <bpmn:sequenceFlow id="Flow_DerogYes" name="Yes" sourceRef="Gateway_Derogatory" targetRef="Task_ManageDerogatory" />
    <bpmn:sequenceFlow id="Flow_DerogResolved" sourceRef="Task_ManageDerogatory" targetRef="Gateway_Derogatory" />
    <bpmn:sequenceFlow id="Flow_DerogNo" name="No" sourceRef="Gateway_Derogatory" targetRef="Task_ReviewAuthUser" />
    <bpmn:sequenceFlow id="Flow_AuthUserEnd" sourceRef="Task_ReviewAuthUser" targetRef="EndEvent_CreditCleared" />
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_Credit">
    <bpmndi:BPMNPlane id="BPMNPlane_Credit" bpmnElement="Collaboration_CreditReview">
