      <bpmndi:BPMNShape id="Task_InquiryLOX_di" bpmnElement="Task_ReviewInquiryLOX">
        <dc:Bounds x="695" y="340" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_Derog_di" bpmnElement="Gateway_Derogatory" isMarkerVisible="true">
