        <bpmndi:BPMNLabel>
          <dc:Bounds x="882" y="118" width="87" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ManageDerog_di" bpmnElement="Task_ManageDerogatory">
        <dc:Bounds x="875" y="340" width="100" height="80" />
