<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:bioc="http://bpmn.io/schema/bpmn/biocolor/1.0" xmlns:color="http://www.omg.org/spec/BPMN/non-normative/color/1.0" id="Definitions_UWInit_Fixed" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.20.0">
  <bpmn:collaboration id="Collaboration_UWInit_Fixed">
    <bpmn:participant id="Participant_UWInit" name="Underwriting - Initial Review (L2)" processRef="Process_UWInit_Fixed" />
  </bpmn:collaboration>
  <bpmn:process id="Process_UWInit_Fixed" name="Initial Review" isExecutable="true">
    <bpmn:laneSet id="LaneSet_UWInit">
      <bpmn:lane id="Lane_System_UW" name="System (Automation)">
        <bpmn:flowNodeRef>Start_FileRec</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GenLetter</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GenDenial</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_CondApp</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Denied</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Suspense</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Human_UW" name="Underwriter (Human)">
        <bpmn:flowNodeRef>Task_ReviewFraud</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReviewCredit</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Decision</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <bpmn:startEvent id="Start_FileRec" name="File Received">
      <bpmn:outgoing>Flow_Start_Fraud</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_ReviewFraud" name="Review Fraud Guard">
      <bpmn:incoming>Flow_Start_Fraud</bpmn:incoming>
      <bpmn:outgoing>Flow_Fraud_Credit</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_ReviewCredit" name="Review Credit (Evolve)">
      <bpmn:incoming>Flow_Fraud_Credit</bpmn:incoming>
      <bpmn:outgoing>Flow_Credit_Dec</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Task_Decision" name="Issue Decision (UW-1A)">
      <bpmn:incoming>Flow_Credit_Dec</bpmn:incoming>
      <bpmn:outgoing>Flow_Dec_Gate</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_Decision" name="Decision?">
      <bpmn:incoming>Flow_Dec_Gate</bpmn:incoming>
      <bpmn:outgoing>Flow_Dec_Approve</bpmn:outgoing>
      <bpmn:outgoing>Flow_Dec_Suspend</bpmn:outgoing>
      <bpmn:outgoing>Flow_Dec_Deny</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:serviceTask id="Task_GenLetter" name="Generate Approval Letter">
      <bpmn:incoming>Flow_Dec_Approve</bpmn:incoming>
      <bpmn:outgoing>Flow_Let_End</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_GenDenial" name="Generate Denial (NOAA)">
      <bpmn:incoming>Flow_Dec_Deny</bpmn:incoming>
      <bpmn:outgoing>Flow_Deny_End</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="End_CondApp" name="Conditional Approval">
      <bpmn:incoming>Flow_Let_End</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="End_Denied" name="Loan Denied">
      <bpmn:incoming>Flow_Deny_End</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="End_Suspense" name="File Suspended">
      <bpmn:incoming>Flow_Dec_Suspend</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_Fraud" sourceRef="Start_FileRec" targetRef="Task_ReviewFraud" />
    <bpmn:sequenceFlow id="Flow_Fraud_Credit" sourceRef="Task_ReviewFraud" targetRef="Task_ReviewCredit" />
    <bpmn:sequenceFlow id="Flow_Credit_Dec" sourceRef="Task_ReviewCredit" targetRef="Task_Decision" />
    <bpmn:sequenceFlow id="Flow_Dec_Gate" sourceRef="Task_Decision" targetRef="Gateway_Decision" />
    <bpmn:sequenceFlow id="Flow_Dec_Approve" sourceRef="Gateway_Decision" targetRef="Task_GenLetter" />
    <bpmn:sequenceFlow id="Flow_Dec_Suspend" sourceRef="Gateway_Decision" targetRef="End_Suspense" />
    <bpmn:sequenceFlow id="Flow_Dec_Deny" sourceRef="Gateway_Decision" targetRef="Task_GenDenial" />
    <bpmn:sequenceFlow id="Flow_Let_End" sourceRef="Task_GenLetter" targetRef="End_CondApp" />
    <bpmn:sequenceFlow id="Flow_Deny_End" sourceRef="Task_GenDenial" targetRef="End_Denied" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_UWInit_Fixed">
    <bpmndi:BPMNPlane id="BPMNPlane_UWInit_Fixed" bpmnElement="Collaboration_UWInit_Fixed">
      <bpmndi:BPMNShape id="Part_di" bpmnElement="Participant_UWInit" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1000" height="400" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Sys_di" bpmnElement="Lane_System_UW" isHorizontal="true" bioc:stroke="#43a047" bioc:fill="#c8e6c9">
        <dc:Bounds x="190" y="80" width="970" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Hum_di" bpmnElement="Lane_Human_UW" isHorizontal="true" bioc:stroke="#fb8c00" bioc:fill="#ffe0b2">
        <dc:Bounds x="190" y="280" width="970" height="200" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Start_di" bpmnElement="Start_FileRec">
        <dc:Bounds x="232" y="162" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Fraud_di" bpmnElement="Task_ReviewFraud">
        <dc:Bounds x="300" y="340" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Cred_di" bpmnElement="Task_ReviewCredit">
        <dc:Bounds x="450" y="340" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Dec_di" bpmnElement="Task_Decision">
        <dc:Bounds x="600" y="340" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gate_Dec_di" bpmnElement="Gateway_Decision" isMarkerVisible="true">
        <dc:Bounds x="750" y="355" width="50" height="50" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Let_di" bpmnElement="Task_GenLetter">
        <dc:Bounds x="850" y="100" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Deny_di" bpmnElement="Task_GenDenial">
        <dc:Bounds x="850" y="200" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="End_App_di" bpmnElement="End_CondApp">
        <dc:Bounds x="1000" y="122" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="End_Deny_di" bpmnElement="End_Denied">
        <dc:Bounds x="1000" y="222" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="End_Sus_di" bpmnElement="End_Suspense">
        <dc:Bounds x="850" y="362" width="36" height="36" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="Flow_1" bpmnElement="Flow_Start_Fraud">
        <di:waypoint x="250" y="198" />
        <di:waypoint x="250" y="380" />
        <di:waypoint x="300" y="380" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_2" bpmnElement="Flow_Fraud_Credit">
        <di:waypoint x="400" y="380" />
        <di:waypoint x="450" y="380" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_3" bpmnElement="Flow_Credit_Dec">
        <di:waypoint x="550" y="380" />
        <di:waypoint x="600" y="380" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_4" bpmnElement="Flow_Dec_Gate">
        <di:waypoint x="700" y="380" />
        <di:waypoint x="750" y="380" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_5" bpmnElement="Flow_Dec_Approve">
        <di:waypoint x="775" y="355" />
        <di:waypoint x="775" y="140" />
        <di:waypoint x="850" y="140" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_6" bpmnElement="Flow_Dec_Deny">
        <di:waypoint x="775" y="355" />
        <di:waypoint x="775" y="240" />
        <di:waypoint x="850" y="240" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_7" bpmnElement="Flow_Dec_Suspend">
        <di:waypoint x="800" y="380" />
        <di:waypoint x="850" y="380" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_8" bpmnElement="Flow_Let_End">
        <di:waypoint x="950" y="140" />
        <di:waypoint x="1000" y="140" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_9" bpmnElement="Flow_Deny_End">
        <di:waypoint x="950" y="240" />
        <di:waypoint x="1000" y="240" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>