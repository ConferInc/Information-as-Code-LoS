<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:zeebe="http://camunda.org/schema/zeebe/1.0"
                  xmlns:modeler="http://camunda.org/schema/modeler/1.0"
                  id="Definitions_L2_DocUpload"
                  targetNamespace="http://bpmn.io/schema/bpmn"
                  exporter="MoXi LOS BPMN Generator"
                  exporterVersion="1.0.0"
                  modeler:executionPlatform="Camunda Cloud"
                  modeler:executionPlatformVersion="8.5.0">

  <!--
    L2 BPMN: Document Upload Flow (WF-06)
    Part of: Application Process Sub-Processes

    This diagram details the document upload workflow for borrowers,
    including file validation, preview, upload to storage, and checklist updates.

    Trigger: Application submitted OR borrower accesses Documents tab
    End State: All required documents uploaded
    Estimated Time: 5-15 minutes per document set

    Preconditions:
    - User logged in
    - Application exists (any status except Draft)
    - Document checklist generated
  -->

  <bpmn:collaboration id="Collaboration_DocUpload">
    <bpmn:participant id="Participant_Borrower" name="Borrower" processRef="Process_Borrower_DocUpload" />
    <bpmn:participant id="Participant_System" name="System" processRef="Process_System_DocUpload" />
  </bpmn:collaboration>

  <!-- ==================== BORROWER LANE ==================== -->
  <bpmn:process id="Process_Borrower_DocUpload" isExecutable="false">

    <bpmn:startEvent id="Start_DocUpload" name="Access Documents Tab">
      <bpmn:outgoing>Flow_B1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_NavigateDocs" name="Navigate to Documents Tab">
      <bpmn:incoming>Flow_B1</bpmn:incoming>
      <bpmn:outgoing>Flow_B2</bpmn:outgoing>
    </bpmn:task>

    <bpmn:intermediateCatchEvent id="Event_ViewChecklist" name="View Document Checklist">
      <bpmn:incoming>Flow_S1_Msg</bpmn:incoming>
      <bpmn:incoming>Flow_LoopBack</bpmn:incoming>
      <bpmn:outgoing>Flow_B3</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_ViewChecklist" />
    </bpmn:intermediateCatchEvent>

    <bpmn:task id="Task_ClickDocType" name="Click Document Type to Upload">
      <bpmn:incoming>Flow_B3</bpmn:incoming>
      <bpmn:outgoing>Flow_B4</bpmn:outgoing>
    </bpmn:task>

    <bpmn:intermediateCatchEvent id="Event_SeeUploadUI" name="View Upload Interface">
      <bpmn:incoming>Flow_S2_Msg</bpmn:incoming>
      <bpmn:outgoing>Flow_B5</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_SeeUploadUI" />
    </bpmn:intermediateCatchEvent>

    <bpmn:task id="Task_SelectFile" name="Select File(s) to Upload">
      <bpmn:documentation>
        Options available:
        - Drag and drop
        - File picker
        - Camera (mobile)
      </bpmn:documentation>
      <bpmn:incoming>Flow_B5</bpmn:incoming>
      <bpmn:incoming>Flow_RetrySelect</bpmn:incoming>
      <bpmn:outgoing>Flow_B6</bpmn:outgoing>
    </bpmn:task>

    <bpmn:intermediateCatchEvent id="Event_SeeValidation" name="Receive Validation Result">
      <bpmn:incoming>Flow_S3_Valid</bpmn:incoming>
      <bpmn:incoming>Flow_S3_Invalid</bpmn:incoming>
      <bpmn:outgoing>Flow_B7</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_Validation" />
    </bpmn:intermediateCatchEvent>

    <bpmn:exclusiveGateway id="Gateway_FileValid" name="File Valid?">
      <bpmn:incoming>Flow_B7</bpmn:incoming>
      <bpmn:outgoing>Flow_Valid</bpmn:outgoing>
      <bpmn:outgoing>Flow_Invalid</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:task id="Task_ViewError" name="View Error Message">
      <bpmn:documentation>
        Possible errors:
        - "Please upload PDF, JPG, or PNG"
        - "File must be under 20MB"
      </bpmn:documentation>
      <bpmn:incoming>Flow_Invalid</bpmn:incoming>
      <bpmn:outgoing>Flow_RetrySelect</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_ViewPreview" name="Review File Preview">
      <bpmn:incoming>Flow_Valid</bpmn:incoming>
      <bpmn:outgoing>Flow_B8</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_ConfirmUpload" name="Click Upload Button">
      <bpmn:incoming>Flow_B8</bpmn:incoming>
      <bpmn:outgoing>Flow_B9</bpmn:outgoing>
    </bpmn:task>

    <bpmn:intermediateCatchEvent id="Event_UploadResult" name="Receive Upload Result">
      <bpmn:incoming>Flow_S5_Success</bpmn:incoming>
      <bpmn:incoming>Flow_S5_Fail</bpmn:incoming>
      <bpmn:outgoing>Flow_B10</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_UploadResult" />
    </bpmn:intermediateCatchEvent>

    <bpmn:exclusiveGateway id="Gateway_UploadSuccess" name="Upload Successful?">
      <bpmn:incoming>Flow_B10</bpmn:incoming>
      <bpmn:outgoing>Flow_Success</bpmn:outgoing>
      <bpmn:outgoing>Flow_UploadFail</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:task id="Task_ViewUploadError" name="View Upload Error">
      <bpmn:documentation>
        "Upload failed. Please try again."
        Shows retry button.
      </bpmn:documentation>
      <bpmn:incoming>Flow_UploadFail</bpmn:incoming>
      <bpmn:outgoing>Flow_RetryUpload</bpmn:outgoing>
    </bpmn:task>

    <bpmn:intermediateCatchEvent id="Event_SuccessMsg" name="View Success Message">
      <bpmn:incoming>Flow_S6_Msg</bpmn:incoming>
      <bpmn:outgoing>Flow_B11</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_Success" />
    </bpmn:intermediateCatchEvent>

    <bpmn:exclusiveGateway id="Gateway_MoreDocs" name="More Documents Needed?">
      <bpmn:incoming>Flow_B11</bpmn:incoming>
      <bpmn:outgoing>Flow_MoreDocs</bpmn:outgoing>
      <bpmn:outgoing>Flow_AllComplete</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:endEvent id="End_AllDocsUploaded" name="All Documents Uploaded">
      <bpmn:incoming>Flow_AllComplete</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Borrower Flow Connections -->
    <bpmn:sequenceFlow id="Flow_B1" sourceRef="Start_DocUpload" targetRef="Task_NavigateDocs" />
    <bpmn:sequenceFlow id="Flow_B2" sourceRef="Task_NavigateDocs" targetRef="Task_RequestChecklist" />
    <bpmn:sequenceFlow id="Flow_B3" sourceRef="Event_ViewChecklist" targetRef="Task_ClickDocType" />
    <bpmn:sequenceFlow id="Flow_B4" sourceRef="Task_ClickDocType" targetRef="Task_RequestUploadUI" />
    <bpmn:sequenceFlow id="Flow_B5" sourceRef="Event_SeeUploadUI" targetRef="Task_SelectFile" />
    <bpmn:sequenceFlow id="Flow_B6" sourceRef="Task_SelectFile" targetRef="Task_SendFileForValidation" />
    <bpmn:sequenceFlow id="Flow_B7" sourceRef="Event_SeeValidation" targetRef="Gateway_FileValid" />
    <bpmn:sequenceFlow id="Flow_Valid" name="Yes" sourceRef="Gateway_FileValid" targetRef="Task_ViewPreview" />
    <bpmn:sequenceFlow id="Flow_Invalid" name="No" sourceRef="Gateway_FileValid" targetRef="Task_ViewError" />
    <bpmn:sequenceFlow id="Flow_RetrySelect" sourceRef="Task_ViewError" targetRef="Task_SelectFile" />
    <bpmn:sequenceFlow id="Flow_B8" sourceRef="Task_ViewPreview" targetRef="Task_ConfirmUpload" />
    <bpmn:sequenceFlow id="Flow_B9" sourceRef="Task_ConfirmUpload" targetRef="Task_InitiateUpload" />
    <bpmn:sequenceFlow id="Flow_B10" sourceRef="Event_UploadResult" targetRef="Gateway_UploadSuccess" />
    <bpmn:sequenceFlow id="Flow_Success" name="Yes" sourceRef="Gateway_UploadSuccess" targetRef="Event_SuccessMsg" />
    <bpmn:sequenceFlow id="Flow_UploadFail" name="No" sourceRef="Gateway_UploadSuccess" targetRef="Task_ViewUploadError" />
    <bpmn:sequenceFlow id="Flow_RetryUpload" sourceRef="Task_ViewUploadError" targetRef="Task_InitiateUpload" />
    <bpmn:sequenceFlow id="Flow_B11" sourceRef="Event_SuccessMsg" targetRef="Gateway_MoreDocs" />
    <bpmn:sequenceFlow id="Flow_MoreDocs" name="Yes" sourceRef="Gateway_MoreDocs" targetRef="Event_ViewChecklist" />
    <bpmn:sequenceFlow id="Flow_AllComplete" name="No" sourceRef="Gateway_MoreDocs" targetRef="End_AllDocsUploaded" />
    <bpmn:sequenceFlow id="Flow_LoopBack" sourceRef="Gateway_MoreDocs" targetRef="Event_ViewChecklist" />

    <!-- Message Send Tasks (to System) -->
    <bpmn:sendTask id="Task_RequestChecklist" name="Request Document Checklist">
      <bpmn:incoming>Flow_B2</bpmn:incoming>
      <bpmn:outgoing>Flow_ToSystem1</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:sendTask id="Task_RequestUploadUI" name="Request Upload Interface">
      <bpmn:incoming>Flow_B4</bpmn:incoming>
      <bpmn:outgoing>Flow_ToSystem2</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:sendTask id="Task_SendFileForValidation" name="Send File for Validation">
      <bpmn:incoming>Flow_B6</bpmn:incoming>
      <bpmn:outgoing>Flow_ToSystem3</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:sendTask id="Task_InitiateUpload" name="Initiate File Upload">
      <bpmn:incoming>Flow_B9</bpmn:incoming>
      <bpmn:incoming>Flow_RetryUpload</bpmn:incoming>
      <bpmn:outgoing>Flow_ToSystem4</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:sequenceFlow id="Flow_ToSystem1" sourceRef="Task_RequestChecklist" targetRef="Event_ViewChecklist" />
    <bpmn:sequenceFlow id="Flow_ToSystem2" sourceRef="Task_RequestUploadUI" targetRef="Event_SeeUploadUI" />
    <bpmn:sequenceFlow id="Flow_ToSystem3" sourceRef="Task_SendFileForValidation" targetRef="Event_SeeValidation" />
    <bpmn:sequenceFlow id="Flow_ToSystem4" sourceRef="Task_InitiateUpload" targetRef="Event_UploadResult" />

  </bpmn:process>

  <!-- ==================== SYSTEM LANE ==================== -->
  <bpmn:process id="Process_System_DocUpload" isExecutable="true">

    <bpmn:startEvent id="Start_System_DocUpload" name="Receive Checklist Request">
      <bpmn:outgoing>Flow_S1</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_StartSystem" />
    </bpmn:startEvent>

    <bpmn:serviceTask id="Task_LoadChecklist" name="Load Document Checklist">
      <bpmn:documentation>
        Query document_requests table for application.
        Group by category, show status for each item.
      </bpmn:documentation>
      <bpmn:incoming>Flow_S1</bpmn:incoming>
      <bpmn:outgoing>Flow_S1a</bpmn:outgoing>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="load-document-checklist" />
      </bpmn:extensionElements>
    </bpmn:serviceTask>

    <bpmn:sendTask id="Task_DisplayChecklist" name="Display Document Checklist">
      <bpmn:documentation>
        Group by category:
        - Income Verification
        - Asset Documentation
        - Identity Documents
        - Property Documents
        Show status: Required, Submitted, Approved, Rejected
      </bpmn:documentation>
      <bpmn:incoming>Flow_S1a</bpmn:incoming>
      <bpmn:incoming>Flow_UpdateComplete</bpmn:incoming>
      <bpmn:outgoing>Flow_S1_Msg</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:intermediateCatchEvent id="Event_ReceiveDocTypeClick" name="Receive Document Type Selection">
      <bpmn:incoming>Flow_S1_Msg</bpmn:incoming>
      <bpmn:outgoing>Flow_S2</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_DocTypeClick" />
    </bpmn:intermediateCatchEvent>

    <bpmn:sendTask id="Task_ShowUploadUI" name="Display Upload Interface">
      <bpmn:documentation>
        Show upload options:
        - Drag and drop zone
        - File picker button
        - Camera capture (mobile)

        Display accepted formats: PDF, JPG, PNG
        Max file size: 20MB
      </bpmn:documentation>
      <bpmn:incoming>Flow_S2</bpmn:incoming>
      <bpmn:outgoing>Flow_S2_Msg</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:intermediateCatchEvent id="Event_ReceiveFile" name="Receive File for Validation">
      <bpmn:incoming>Flow_S2_Msg</bpmn:incoming>
      <bpmn:outgoing>Flow_S3</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_ReceiveFile" />
    </bpmn:intermediateCatchEvent>

    <bpmn:serviceTask id="Task_ValidateFile" name="Validate File">
      <bpmn:documentation>
        Validation rules:
        - Type: PDF, JPG, PNG only
        - Size: Max 20MB
        - Content: Basic integrity check
      </bpmn:documentation>
      <bpmn:incoming>Flow_S3</bpmn:incoming>
      <bpmn:outgoing>Flow_S3a</bpmn:outgoing>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="validate-document-file" />
      </bpmn:extensionElements>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_ValidationResult" name="Validation Passed?">
      <bpmn:incoming>Flow_S3a</bpmn:incoming>
      <bpmn:outgoing>Flow_ValidationPass</bpmn:outgoing>
      <bpmn:outgoing>Flow_ValidationFail</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:sendTask id="Task_SendValidationError" name="Send Validation Error">
      <bpmn:incoming>Flow_ValidationFail</bpmn:incoming>
      <bpmn:outgoing>Flow_S3_Invalid</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:serviceTask id="Task_GeneratePreview" name="Generate File Preview">
      <bpmn:documentation>
        Create thumbnail/preview of uploaded file.
        For PDFs: First page preview
        For images: Scaled thumbnail
      </bpmn:documentation>
      <bpmn:incoming>Flow_ValidationPass</bpmn:incoming>
      <bpmn:outgoing>Flow_S3b</bpmn:outgoing>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="generate-document-preview" />
      </bpmn:extensionElements>
    </bpmn:serviceTask>

    <bpmn:sendTask id="Task_SendPreview" name="Send Preview to Borrower">
      <bpmn:incoming>Flow_S3b</bpmn:incoming>
      <bpmn:outgoing>Flow_S3_Valid</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:intermediateCatchEvent id="Event_ReceiveUploadConfirm" name="Receive Upload Confirmation">
      <bpmn:incoming>Flow_S3_Valid</bpmn:incoming>
      <bpmn:outgoing>Flow_S4</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MsgDef_UploadConfirm" />
    </bpmn:intermediateCatchEvent>

    <bpmn:serviceTask id="Task_UploadToStorage" name="Upload to Storage">
      <bpmn:documentation>
        Upload file to Supabase Storage.
        Show progress indicator to user.
        Auto-retry up to 3 times on failure.
      </bpmn:documentation>
      <bpmn:incoming>Flow_S4</bpmn:incoming>
      <bpmn:outgoing>Flow_S4a</bpmn:outgoing>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="upload-to-storage" />
      </bpmn:extensionElements>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_UploadResult" name="Upload Successful?">
      <bpmn:incoming>Flow_S4a</bpmn:incoming>
      <bpmn:outgoing>Flow_UploadOK</bpmn:outgoing>
      <bpmn:outgoing>Flow_UploadError</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:sendTask id="Task_SendUploadError" name="Send Upload Error">
      <bpmn:documentation>
        Error messages:
        - "Upload failed. Please try again."
        - "Connection lost. Retrying..."
      </bpmn:documentation>
      <bpmn:incoming>Flow_UploadError</bpmn:incoming>
      <bpmn:outgoing>Flow_S5_Fail</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:serviceTask id="Task_CreateDocRecord" name="Create Document Record">
      <bpmn:documentation>
        Data changes:
        - Create record in documents table
        - Create storage reference in document_assets
        - Link to application and document type
      </bpmn:documentation>
      <bpmn:incoming>Flow_UploadOK</bpmn:incoming>
      <bpmn:outgoing>Flow_S5a</bpmn:outgoing>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="create-document-record" />
      </bpmn:extensionElements>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_UpdateChecklist" name="Update Checklist Status">
      <bpmn:documentation>
        Update document_requests.status = 'submitted'
        Mark checklist item as "Pending Review"
      </bpmn:documentation>
      <bpmn:incoming>Flow_S5a</bpmn:incoming>
      <bpmn:outgoing>Flow_S5b</bpmn:outgoing>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="update-checklist-status" />
      </bpmn:extensionElements>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_LogAuditEvent" name="Log Audit Event">
      <bpmn:documentation>
        Log upload event to audit_log table.
        Include: user_id, application_id, document_type, timestamp
      </bpmn:documentation>
      <bpmn:incoming>Flow_S5b</bpmn:incoming>
      <bpmn:outgoing>Flow_S5c</bpmn:outgoing>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="log-audit-event" />
      </bpmn:extensionElements>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_NotifyLO" name="Notify Loan Officer">
      <bpmn:documentation>
        Send in-app notification to LO dashboard.
        "New document received for [Application ID]"
      </bpmn:documentation>
      <bpmn:incoming>Flow_S5c</bpmn:incoming>
      <bpmn:outgoing>Flow_S5d</bpmn:outgoing>
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="notify-loan-officer" />
      </bpmn:extensionElements>
    </bpmn:serviceTask>

    <bpmn:sendTask id="Task_SendSuccessMsg" name="Send Success Message">
      <bpmn:documentation>
        Display: "Document uploaded successfully"
        Show updated checklist with new status.
      </bpmn:documentation>
      <bpmn:incoming>Flow_S5d</bpmn:incoming>
      <bpmn:outgoing>Flow_S5_Success</bpmn:outgoing>
      <bpmn:outgoing>Flow_UpdateComplete</bpmn:outgoing>
    </bpmn:sendTask>

    <!-- System Flow Connections -->
    <bpmn:sequenceFlow id="Flow_S1" sourceRef="Start_System_DocUpload" targetRef="Task_LoadChecklist" />
    <bpmn:sequenceFlow id="Flow_S1a" sourceRef="Task_LoadChecklist" targetRef="Task_DisplayChecklist" />
    <bpmn:sequenceFlow id="Flow_S1_Msg" sourceRef="Task_DisplayChecklist" targetRef="Event_ReceiveDocTypeClick" />
    <bpmn:sequenceFlow id="Flow_S2" sourceRef="Event_ReceiveDocTypeClick" targetRef="Task_ShowUploadUI" />
    <bpmn:sequenceFlow id="Flow_S2_Msg" sourceRef="Task_ShowUploadUI" targetRef="Event_ReceiveFile" />
    <bpmn:sequenceFlow id="Flow_S3" sourceRef="Event_ReceiveFile" targetRef="Task_ValidateFile" />
    <bpmn:sequenceFlow id="Flow_S3a" sourceRef="Task_ValidateFile" targetRef="Gateway_ValidationResult" />
    <bpmn:sequenceFlow id="Flow_ValidationPass" name="Yes" sourceRef="Gateway_ValidationResult" targetRef="Task_GeneratePreview" />
    <bpmn:sequenceFlow id="Flow_ValidationFail" name="No" sourceRef="Gateway_ValidationResult" targetRef="Task_SendValidationError" />
    <bpmn:sequenceFlow id="Flow_S3_Invalid" sourceRef="Task_SendValidationError" targetRef="Event_ReceiveFile" />
    <bpmn:sequenceFlow id="Flow_S3b" sourceRef="Task_GeneratePreview" targetRef="Task_SendPreview" />
    <bpmn:sequenceFlow id="Flow_S3_Valid" sourceRef="Task_SendPreview" targetRef="Event_ReceiveUploadConfirm" />
    <bpmn:sequenceFlow id="Flow_S4" sourceRef="Event_ReceiveUploadConfirm" targetRef="Task_UploadToStorage" />
    <bpmn:sequenceFlow id="Flow_S4a" sourceRef="Task_UploadToStorage" targetRef="Gateway_UploadResult" />
    <bpmn:sequenceFlow id="Flow_UploadOK" name="Yes" sourceRef="Gateway_UploadResult" targetRef="Task_CreateDocRecord" />
    <bpmn:sequenceFlow id="Flow_UploadError" name="No" sourceRef="Gateway_UploadResult" targetRef="Task_SendUploadError" />
    <bpmn:sequenceFlow id="Flow_S5_Fail" sourceRef="Task_SendUploadError" targetRef="Event_ReceiveFile" />
    <bpmn:sequenceFlow id="Flow_S5a" sourceRef="Task_CreateDocRecord" targetRef="Task_UpdateChecklist" />
    <bpmn:sequenceFlow id="Flow_S5b" sourceRef="Task_UpdateChecklist" targetRef="Task_LogAuditEvent" />
    <bpmn:sequenceFlow id="Flow_S5c" sourceRef="Task_LogAuditEvent" targetRef="Task_NotifyLO" />
    <bpmn:sequenceFlow id="Flow_S5d" sourceRef="Task_NotifyLO" targetRef="Task_SendSuccessMsg" />
    <bpmn:sequenceFlow id="Flow_S5_Success" sourceRef="Task_SendSuccessMsg" targetRef="Event_ReceiveDocTypeClick" />
    <bpmn:sequenceFlow id="Flow_UpdateComplete" sourceRef="Task_SendSuccessMsg" targetRef="Task_DisplayChecklist" />

  </bpmn:process>

  <!-- ==================== DIAGRAM LAYOUT ==================== -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_DocUpload">
    <bpmndi:BPMNPlane id="BPMNPlane_DocUpload" bpmnElement="Collaboration_DocUpload">

      <!-- Borrower Pool -->
      <bpmndi:BPMNShape id="Participant_Borrower_di" bpmnElement="Participant_Borrower" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1800" height="350" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- System Pool -->
      <bpmndi:BPMNShape id="Participant_System_di" bpmnElement="Participant_System" isHorizontal="true">
        <dc:Bounds x="160" y="450" width="1800" height="400" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Borrower Elements -->
      <bpmndi:BPMNShape id="Start_DocUpload_di" bpmnElement="Start_DocUpload">
        <dc:Bounds x="222" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="225" width="80" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_NavigateDocs_di" bpmnElement="Task_NavigateDocs">
        <dc:Bounds x="300" y="160" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_RequestChecklist_di" bpmnElement="Task_RequestChecklist">
        <dc:Bounds x="440" y="160" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Event_ViewChecklist_di" bpmnElement="Event_ViewChecklist">
        <dc:Bounds x="582" y="182" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="560" y="140" width="80" height="40" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_ClickDocType_di" bpmnElement="Task_ClickDocType">
        <dc:Bounds x="660" y="160" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

