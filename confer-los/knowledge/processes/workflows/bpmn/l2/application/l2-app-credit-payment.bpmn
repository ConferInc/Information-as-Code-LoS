<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:bioc="http://bpmn.io/schema/bpmn/biocolor/1.0" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_CreditPayment" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.43.1" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.4.0">
  <bpmn:collaboration id="Collaboration_Payment">
    <bpmn:participant id="Participant_Payment" name="Credit Report Fee Payment (L2)" processRef="Process_Payment_L2" />
  </bpmn:collaboration>
  <bpmn:process id="Process_Payment_L2" name="Payment Process" isExecutable="true">
    <bpmn:laneSet id="LaneSet_Payment">
      <bpmn:lane id="Lane_System_Pay" name="System (Stripe/Payment Gateway)">
        <bpmn:flowNodeRef>Gateway_MethodCheck</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Success</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ProcessCharge</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_LogFailure</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Borrower" name="Borrower (Frontend)">
        <bpmn:flowNodeRef>StartEvent_PaymentReq</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SelectMethod</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EnterCard</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RetryPayment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Paid</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_PaymentReq" name="Payment Requested">
      <bpmn:outgoing>Flow_StartToSelect</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_SelectMethod" name="Select Payment Method">
      <bpmn:documentation>Dropdown: Credit Card vs ACH.</bpmn:documentation>
      <bpmn:incoming>Flow_StartToSelect</bpmn:incoming>
      <bpmn:outgoing>Flow_SelectToGate</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_MethodCheck" name="Is Credit Card?">
      <bpmn:incoming>Flow_SelectToGate</bpmn:incoming>
      <bpmn:outgoing>Flow_MethodCC</bpmn:outgoing>
      <bpmn:outgoing>Flow_MethodOther</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_EnterCard" name="Enter Card Details">
      <bpmn:documentation>First, Last, Card Number, CVV.</bpmn:documentation>
      <bpmn:incoming>Flow_MethodCC</bpmn:incoming>
      <bpmn:incoming>Flow_RetryToEnter</bpmn:incoming>
      <bpmn:outgoing>Flow_EnterToProcess</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_Success" name="Payment Successful?">
      <bpmn:incoming>Flow_ProcessToSuccess</bpmn:incoming>
      <bpmn:outgoing>Flow_SuccessYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_SuccessNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_RetryPayment" name="Retry Payment">
      <bpmn:documentation>Show error message to user.</bpmn:documentation>
      <bpmn:incoming>Flow_LogToRetry</bpmn:incoming>
      <bpmn:outgoing>Flow_RetryToEnter</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_Paid" name="Fee Paid">
      <bpmn:incoming>Flow_SuccessYes</bpmn:incoming>
      <bpmn:incoming>Flow_MethodOther</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:serviceTask id="Task_ProcessCharge" name="Process Charge (Stripe)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="process-stripe-payment" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_EnterToProcess</bpmn:incoming>
      <bpmn:outgoing>Flow_ProcessToSuccess</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_LogFailure" name="Log Failure">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="log-payment-failure" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_SuccessNo</bpmn:incoming>
      <bpmn:outgoing>Flow_LogToRetry</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_StartToSelect" sourceRef="StartEvent_PaymentReq" targetRef="Task_SelectMethod" />
    <bpmn:sequenceFlow id="Flow_SelectToGate" sourceRef="Task_SelectMethod" targetRef="Gateway_MethodCheck" />
    <bpmn:sequenceFlow id="Flow_MethodCC" name="Credit Card" sourceRef="Gateway_MethodCheck" targetRef="Task_EnterCard" />
    <bpmn:sequenceFlow id="Flow_MethodOther" name="Other (Invoice)" sourceRef="Gateway_MethodCheck" targetRef="EndEvent_Paid" />
    <bpmn:sequenceFlow id="Flow_RetryToEnter" sourceRef="Task_RetryPayment" targetRef="Task_EnterCard" />
    <bpmn:sequenceFlow id="Flow_EnterToProcess" sourceRef="Task_EnterCard" targetRef="Task_ProcessCharge" />
    <bpmn:sequenceFlow id="Flow_ProcessToSuccess" sourceRef="Task_ProcessCharge" targetRef="Gateway_Success" />
    <bpmn:sequenceFlow id="Flow_SuccessYes" name="Yes" sourceRef="Gateway_Success" targetRef="EndEvent_Paid" />
    <bpmn:sequenceFlow id="Flow_SuccessNo" name="No (Declined)" sourceRef="Gateway_Success" targetRef="Task_LogFailure" />
    <bpmn:sequenceFlow id="Flow_LogToRetry" sourceRef="Task_LogFailure" targetRef="Task_RetryPayment" />
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_Payment">
    <bpmndi:BPMNPlane id="BPMNPlane_Payment" bpmnElement="Collaboration_Payment">
      <bpmndi:BPMNShape id="Participant_Pay_di" bpmnElement="Participant_Payment" isHorizontal="true">
        <dc:Bounds x="165" y="80" width="1015" height="400" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_System_Pay_di" bpmnElement="Lane_System_Pay" isHorizontal="true" bioc:stroke="#43a047" bioc:fill="#c8e6c9">
        <dc:Bounds x="195" y="280" width="985" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Borrower_di" bpmnElement="Lane_Borrower" isHorizontal="true" bioc:stroke="#fb8c00" bioc:fill="#ffe0b2">
        <dc:Bounds x="195" y="80" width="985" height="200" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Start_di" bpmnElement="StartEvent_PaymentReq">
        <dc:Bounds x="255" y="160" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="246" y="196" width="54" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Select_di" bpmnElement="Task_SelectMethod">
        <dc:Bounds x="355" y="140" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gate_Method_di" bpmnElement="Gateway_MethodCheck" isMarkerVisible="true">
        <dc:Bounds x="505" y="300" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="564.5" y="318" width="75" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Enter_di" bpmnElement="Task_EnterCard">
        <dc:Bounds x="605" y="140" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gate_Success_di" bpmnElement="Gateway_Success" isMarkerVisible="true">
        <dc:Bounds x="905" y="315" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="900" y="365" width="60" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Retry_di" bpmnElement="Task_RetryPayment">
        <dc:Bounds x="755" y="140" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="End_di" bpmnElement="EndEvent_Paid">
        <dc:Bounds x="1055" y="162" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1050" y="138" width="45" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Process_di" bpmnElement="Task_ProcessCharge">
        <dc:Bounds x="755" y="300" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_Log_di" bpmnElement="Task_LogFailure">
        <dc:Bounds x="1005" y="300" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_1" bpmnElement="Flow_StartToSelect">
        <di:waypoint x="291" y="178" />
        <di:waypoint x="355" y="178" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_2" bpmnElement="Flow_SelectToGate">
        <di:waypoint x="405" y="220" />
        <di:waypoint x="405" y="325" />
        <di:waypoint x="505" y="325" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_3" bpmnElement="Flow_MethodCC">
        <di:waypoint x="530" y="300" />
        <di:waypoint x="530" y="180" />
        <di:waypoint x="605" y="180" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="517" y="230" width="57" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_4" bpmnElement="Flow_MethodOther">
        <di:waypoint x="530" y="350" />
        <di:waypoint x="530" y="460" />
        <di:waypoint x="1125" y="460" />
        <di:waypoint x="1125" y="180" />
        <di:waypoint x="1091" y="180" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="790" y="435" width="74" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_10" bpmnElement="Flow_RetryToEnter">
        <di:waypoint x="755" y="180" />
        <di:waypoint x="705" y="180" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_5" bpmnElement="Flow_EnterToProcess">
        <di:waypoint x="655" y="220" />
        <di:waypoint x="655" y="340" />
        <di:waypoint x="755" y="340" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_6" bpmnElement="Flow_ProcessToSuccess">
        <di:waypoint x="855" y="340" />
        <di:waypoint x="905" y="340" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_7" bpmnElement="Flow_SuccessYes">
        <di:waypoint x="930" y="315" />
        <di:waypoint x="930" y="180" />
        <di:waypoint x="1055" y="180" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="936" y="238" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_8" bpmnElement="Flow_SuccessNo">
        <di:waypoint x="955" y="340" />
        <di:waypoint x="1005" y="340" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="949" y="323" width="68" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_9" bpmnElement="Flow_LogToRetry">
        <di:waypoint x="1105" y="340" />
        <di:waypoint x="1155" y="340" />
        <di:waypoint x="1155" y="110" />
        <di:waypoint x="805" y="110" />
        <di:waypoint x="805" y="140" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
